####
# Example dotenv file
# Rename to .env and change settings for deploytment
# Do not include this file (or .env) in docker builds
####
# Development Settings
IS_PROD=<False until True>  # guards the prod database
EVENT_NAME=<name of the event>
EVENT_CONTACT_EMAIL=<CONTACT EMAIL FOR SUPPORT>

# Discord IDs (find these in Discord developer mode)
EVENT_GUILD_ID=<YOUR GUILD ID>
EVENT_BOT_CHANNEL_ID=<THE BOT CHANNEL ID>
GUILD_OWNER_ID=<OWNER ID>
BOT_MANAGER_ROLE_ID=<ROLE WITH PERMISSIONS TO MANAGE BOT>

BOT_TOKEN=<YOUR BOT TOKEN>
BOT_PREFIX=!
MARIADB_ROOT_PASSWORD=<MariaDB password for root account>
LOGGING_STR=IC-BOT-DISCORD